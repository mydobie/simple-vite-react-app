{
  "name": "simple-vite-react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "18 || 20"
  },
  "scripts": {
    "dev": " vite",
    "dev:demo": "USE_DEMO=true vite",
    "dev:mock": "USE_MOCK=true vite",
    "build:ci": "vite build",
    "build": "npm run lint && npm run build:ci",
    "build:demo": "npm run lint &&  USE_DEMO=true USE_HASH npm run build:ci ",
    "build:mock": " npm run lint && USE_MOCK=true npm run build:ci",
    "format": "prettier --ignore-path ./.eslintignore --check 'src/**/*.{js,jsx,ts,tsx,css,scss,md,json,html}' ",
    "format:fix": "prettier --ignore-path ./.eslintignore --write 'src/**/*.{js,jsx,ts,tsx,css,scss,md,json,html}' ",
    "lint": "tsc && npm run format && npm run lint:css && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && echo CODE ALL LINTED!",
    "preview": "vite preview",
    "test": "jest ",
    "npmAudit": "npm audit -audit-level high --omit=dev",
    "createTar": "node ./utils/createArchive.js",
    "prepare": "husky install",
    "lint:css": "npm run lint:css:css && npm run lint:css:js",
    "lint:css:css": "stylelint --ignore-path ./.eslintignore './**/*.+(css|scss)'",
    "lint:css:js": "stylelint --ignore-path ./.eslintignore --config .stylelintrcJS './**/*.+(ts|tsx|js|jsx)'",
    "checkCoverage": "node ./utils/testCoverage.js",
    "uglify": "uglifyjs ",
    "cypress:demo:dev": "USE_DEMO=true USE_HASH=false USE_MOCK=false vite",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.17.10",
    "axios": "^1.6.5",
    "bootstrap": "^5.3.2",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.2",
    "react-bootstrap-icons": "^1.10.3",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.21.2",
    "react-router-dom": "^6.21.2",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.11.0",
    "@types/prismjs": "^1.26.3",
    "@types/react": "^18.2.43",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "axe-core": "^4.8.3",
    "axios-mock-adapter": "^1.22.0",
    "cypress": "^13.6.2",
    "cypress-axe": "^1.5.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss-styled-syntax": "^0.6.4",
    "prettier": "^3.2.2",
    "sass": "^1.69.7",
    "stylelint": "^16.1.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "ts-jest": "^29.1.1",
    "ts-jest-mock-import-meta": "^1.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "uglify-js": "^3.17.4",
    "vite": "^5.0.8"
  }
}
